v0 = 10
t = 0.1
q = -0.5
psi = TruncNormal(0, 0.01, -1, 1)
v = Uniform(6.5, 8.0)
x = Uniform(-.1, .1)
y = Uniform(-.5, -.3)
while true:

    w1 = Uniform(-0.1, 0.1)
    w2 = TruncNormal(0, 0.01, -1, 1)

    x = x + t * v * (-8.35716730441716e-11*psi**9 + 1.50429306664942e-5*psi**8 + 9.02574919782205e-9*psi**7 - 0.00126360578561074*psi**6 - 4.1067174862215e-7*psi**5 + 0.0410671821777935*psi**4 + 8.31873628793799e-6*psi**3 - 0.499124193112895*psi**2 - 4.99913940340902e-5*psi + 0.999827884544432)
    y = y + t * v * (1.67143346043521e-6*psi**9 - 0.0001805149839349*psi**7 + 0.00821343497268936*psi**5 - 0.166374725871711*psi**3 + 0.999827881549907*psi)

    v = v + t*(q*(v - v0) + w1)
    psi = psi + w2

end