angle = 0
x = Cos(angle)
y = Sin(angle)
cos_angle = Cos(angle)
sin_angle = Sin(angle)
while true:
    d = 1
    cos_d = Cos(d)
    sin_d = Sin(d)
    angle = angle + d
    sin_angle = sin_angle*cos_d + cos_angle*sin_d
    cos_angle = cos_angle*cos_d - sin_angle*sin_d
    x = cos_angle
    y = sin_angle
end