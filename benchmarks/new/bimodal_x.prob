types
c: Finite(0,1,2)
end

x = 2
xlow = 0
xup = 0
while true:
    c = Categorical(1/3, 1/3, 1/3)
    if c == 0:
        x = Normal(2, 1)
    elif c == 1:
        step = Normal(1, 1)
        xlow = xlow - step
        x = xlow
    else:
        step = Normal(1, 1)
        xup = xup + step
        x = xup
    end
end